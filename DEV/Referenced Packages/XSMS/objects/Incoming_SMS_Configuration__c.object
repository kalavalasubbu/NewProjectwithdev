    <validationRules>
        <fullName>Incoming_Configuration_for_Case</fullName>
        <active>true</active>
        <description>If &quot;Is Case&quot; is selected then Campaign should be null.</description>
        <errorConditionFormula>IF(
         AND(
                   Is_Case__c == true,
                    Campaign__c != null
                ),
          true,
          false
)</errorConditionFormula>
        <errorDisplayField>Campaign__c</errorDisplayField>
        <errorMessage>If Incoming SMS is related to Case then Campaign must not be selected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Incoming_Configuration_for_Case_Who</fullName>
        <active>true</active>
        <description>If &quot;Is Case&quot; is selected then &quot;Who&quot; must be selected as &quot;Contact&quot;</description>
        <errorConditionFormula>IF(
         AND(
                   Is_Case__c == true,
                   TEXT(Who__c) != &apos;Contact&apos;
                ),
          true,
          false
)</errorConditionFormula>
        <errorDisplayField>Who__c</errorDisplayField>
        <errorMessage>If Incoming SMS is related to Case then Who must be selected as Contact.</errorMessage>
    </validationRules>
